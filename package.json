{
  "name": "eulercopilot-client",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "tauri:dev": "RUST_BACKTRACE=1 tauri dev",
    "tauri:release": "tauri build --release",
    "cargo:format": "cd src-tauri && cargo fmt",
    "cargo:check": "cd src-tauri && cargo check",
    "cargo:clean": "cd src-tauri && cargo clean",
    "update": "pnpm update && cd src-tauri && cargo update"
  },
  "dependencies": {
    "@computing/opendesign2": "file:lib/opendesign2-2.0.11.tgz",
    "@tauri-apps/api": "^1.6.0",
    "axios": "1.6.1",
    "dayjs": "1.11.9",
    "echarts": "5.4.3",
    "element-plus": "2.3.12",
    "highlight.js": "11.4.0",
    "marked": "~4.3.0",
    "pinia": "2.1.6",
    "sass": "1.62.0",
    "vue": "^3.5.6",
    "vue-router": "^4.4.5",
    "xss": "1.0.14"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^1.6.2",
    "@vitejs/plugin-vue": "^5.1.4",
    "typescript": "^5.6.2",
    "vite": "^5.4.6",
    "vue-tsc": "^2.1.6"
  }
}
