{
  "productName": "eulercopilot-client",
  "version": "1.0.0",
  "identifier": "org.openeuler.eulercopilot",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "EulerCopilot",
        "decorations": false,
        "alwaysOnTop": true,
        "skipTaskbar": true,
        "width": 800,
        "height": 600,
        "center": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": ["rpm", "appimage", "msi", "dmg"],
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "plugins": {
    "fs": {
      "scope": ["*"]
    }
  }
}
